{
  "compilerOptions": {
    "target": "ES2019",
    "module": "CommonJS",
    "declaration": true,
    "outDir": "./build",
    "rootDir": "./src",
    "downlevelIteration": true,
    "strict": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "forceConsistentCasingInFileNames": false
  },
  "exclude": [
    "src/tests",
    "**/output-models",
    "**/prova.ts",
    "**/tmp"
  ]
}
